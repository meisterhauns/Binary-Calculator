boolean[][] fillRandomValues(boolean[][] n){
    for(boolean[] b : n){
        for (boolean bit : b){
            bit = int2Bool(round(random(0,1)));
        }
    }
    return n;
}

String returnSingleNumber(boolean[] n){
    StringBuilder s = new StringBuilder("");
    for (boolean a : n){
        s.insert(0, bool2Int(a));
    }
    return s.toString();
}
